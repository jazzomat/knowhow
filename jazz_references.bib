@InProceedings{Ozaslan:2010:CMMR,
  author =        {\"{O}zaslan, Tan Hakan and Guaus, Enric and Palacios, Eric and Arcos, Josep Lluis},
  title =         {{Attack Based Articulation Analysis of Nylon String Guitar}},
